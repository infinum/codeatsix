%html
  %head
    %title Code at six
    = stylesheet_link_tag 'application'
    = javascript_include_tag 'application'
    = csrf_meta_tags
    %meta{:name => "viewport", :content => "width=device-width,initial-scale=1.0"}
  %body
    .line-nums
      %ol
    #container
      .banner
        = render :partial => 'layouts/banner'
      .event-date-info
        = render :partial => 'layouts/navigation'
      .content
        = yield

